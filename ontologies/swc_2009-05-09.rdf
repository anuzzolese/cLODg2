    <owl:Class rdf:about="&swc;CallForPosters">
        <rdfs:label>Call for Posters</rdfs:label>
        <rdfs:subClassOf rdf:resource="&swc;Call"/>
        <owl:deprecated rdf:datatype="&xsd;boolean">true</owl:deprecated>
        <rdfs:comment>
			&lt;p&gt;A call for scientific posters at a conference or workshop. Submissions are usually in the form of extended abstracts (~2 page) discussing the research presented on the poster.&lt;/p&gt;
			
		</rdfs:comment>
        <vs:term_status>deprecated</vs:term_status>
        <dcterms:subject rdf:resource="http://dbpedia.org/resource/Call"/>
    </owl:Class>
    


