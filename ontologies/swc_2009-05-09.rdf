<!DOCTYPE rdf:RDF [
    <!ENTITY sioc "http://rdfs.org/sioc/ns#" >
    <!ENTITY dcterms "http://purl.org/dc/terms/" >
    <!ENTITY foaf "http://xmlns.com/foaf/0.1/" >
    <!ENTITY doap "http://usefulinc.com/ns/doap#" >
    <!ENTITY wordnet "http://xmlns.com/wordnet/1.6/" >
    <!ENTITY owl "http://www.w3.org/2002/07/owl#" >
    <!ENTITY dc "http://purl.org/dc/elements/1.1/" >
    <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#" >
    <!ENTITY bibtex "http://swrc.ontoware.org/ontology#" >
    <!ENTITY ical "http://www.w3.org/2002/12/cal/ical#" >
    <!ENTITY misc "http://data.semanticweb.org/ns/misc#" >
    <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#" >
    <!ENTITY geo "http://www.w3.org/2003/01/geo/wgs84_pos#" >
    <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#" >
    <!ENTITY swc "http://data.semanticweb.org/ns/swc/ontology#" >
    <!ENTITY vs "http://www.w3.org/2003/06/sw-vocab-status/ns#" >
    <!ENTITY swrc_ext "http://www.cs.vu.nl/~mcaklein/onto/swrc_ext/2005/05#" >
]>


<rdf:RDF xmlns="http://data.semanticweb.org/ns/swc/ontology#"
     xml:base="http://data.semanticweb.org/ns/swc/ontology"
     xmlns:swrc_ext="http://www.cs.vu.nl/~mcaklein/onto/swrc_ext/2005/05#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:geo="http://www.w3.org/2003/01/geo/wgs84_pos#"
     xmlns:ical="http://www.w3.org/2002/12/cal/ical#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:doap="http://usefulinc.com/ns/doap#"
     xmlns:wordnet="http://xmlns.com/wordnet/1.6/"
     xmlns:dcterms="http://purl.org/dc/terms/"
     xmlns:bibtex="http://swrc.ontoware.org/ontology#"
     xmlns:vs="http://www.w3.org/2003/06/sw-vocab-status/ns#"
     xmlns:swc="http://data.semanticweb.org/ns/swc/ontology#"
     xmlns:foaf="http://xmlns.com/foaf/0.1/"
     xmlns:misc="http://data.semanticweb.org/ns/misc#"
     xmlns:dc="http://purl.org/dc/elements/1.1/"
     xmlns:sioc="http://rdfs.org/sioc/ns#">
    <owl:Ontology rdf:about="http://data.semanticweb.org/ns/swc/ontology">
        <rdfs:label>Semantic Web Conference Ontology</rdfs:label>
        <dcterms:title>Semantic Web Conference Ontology</dcterms:title>
        <dcterms:created>2007/05/31</dcterms:created>
        <doap:revision>
			$Revision: 895 $ - &lt;i&gt;Slim Version&lt;/i&gt;
			</doap:revision>
        <dcterms:modified>$Date: 2009-05-11 17:50:14 +0100 (Mon, 11 May 2009) $</dcterms:modified>
        <dcterms:license rdf:resource="http://creativecommons.org/licenses/by/3.0/"/>
        <foaf:logo rdf:resource="http://data.semanticweb.org/images/bones_256.png"/>
        <dcterms:hasVersion rdf:resource="http://data.semanticweb.org/ns/swc/swc_2009-02-27.rdf"/>
        <sioc:earlier_version rdf:resource="http://data.semanticweb.org/ns/swc/swc_2009-03-02.rdf"/>
        <dcterms:hasVersion rdf:resource="http://data.semanticweb.org/ns/swc/swc_2009-03-02.rdf"/>
        <owl:imports rdf:resource="http://data.semanticweb.org/ns/swc/swrc"/>
        <owl:imports rdf:resource="http://data.semanticweb.org/ns/swc/swrc-topics"/>
        <misc:licenseDoc rdf:resource="&swc;license_doc"/>
        <dcterms:creator rdf:resource="http://data.semanticweb.org/person/sean-bechhofer"/>
        <dcterms:creator rdf:resource="http://kantenwerk.org/metadata/foaf.rdf#me"/>
        <dcterms:creator rdf:resource="http://tomheath.com/id/me"/>
        <swrc_ext:authorList>
            <rdf:Description/>
        </swrc_ext:authorList>
        <vs:userdocs>
            <rdf:Description/>
        </vs:userdocs>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://data.semanticweb.org/ns/swc/ontology#isTopicOf -->

    <owl:AnnotationProperty rdf:about="&swc;isTopicOf">
        <rdfs:label>is topic of</rdfs:label>
        <owl:deprecated rdf:datatype="&xsd;boolean">true</owl:deprecated>
        <rdfs:comment>
			&lt;p&gt;Deprecated, instead using terms from FOAF.&lt;/p&gt;
			
		</rdfs:comment>
        <vs:term_status>deprecated</vs:term_status>
        <rdfs:domain rdf:resource="&bibtex;ResearchTopic"/>
    </owl:AnnotationProperty>
    


    <!-- http://purl.org/dc/elements/1.1/contributor -->

    <owl:AnnotationProperty rdf:about="&dc;contributor"/>
    


    <!-- http://purl.org/dc/elements/1.1/creator -->

    <owl:AnnotationProperty rdf:about="&dc;creator"/>
    


    <!-- http://purl.org/dc/elements/1.1/date -->

    <owl:AnnotationProperty rdf:about="&dc;date"/>
    


    <!-- http://purl.org/dc/elements/1.1/description -->

    <owl:AnnotationProperty rdf:about="&dc;description"/>
    


    <!-- http://purl.org/dc/elements/1.1/title -->

    <owl:AnnotationProperty rdf:about="&dc;title"/>
    


    <!-- http://purl.org/dc/terms/subject -->

    <owl:AnnotationProperty rdf:about="&dcterms;subject"/>
    


    <!-- http://www.w3.org/2003/06/sw-vocab-status/ns#term_status -->

    <owl:AnnotationProperty rdf:about="&vs;term_status"/>
    


    <!-- http://xmlns.com/foaf/0.1/name -->

    <owl:AnnotationProperty rdf:about="&foaf;name"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://data.semanticweb.org/ns/swc/ontology#attendeeAt -->

    <owl:ObjectProperty rdf:about="&swc;attendeeAt">
        <rdfs:label>attendee at</rdfs:label>
        <owl:deprecated rdf:datatype="&xsd;boolean">true</owl:deprecated>
        <rdfs:comment>
			&lt;p&gt;Instead of this property, use the &lt;a href=&quot;#Delegate&quot;&gt;Delegate&lt;/a&gt; role.&lt;/p&gt;
			
		</rdfs:comment>
        <vs:term_status>deprecated</vs:term_status>
        <rdfs:range rdf:resource="&swc;OrganisedEvent"/>
        <owl:inverseOf rdf:resource="&swc;hasAttendee"/>
        <rdfs:domain rdf:resource="&foaf;Person"/>
    </owl:ObjectProperty>
    


    <!-- http://data.semanticweb.org/ns/swc/ontology#forEvent -->

    <owl:ObjectProperty rdf:about="&swc;forEvent">
        <rdfs:label>for event</rdfs:label>
        <owl:deprecated rdf:datatype="&xsd;boolean">true</owl:deprecated>
        <rdfs:comment>
			&lt;p&gt;The call classes have been deprecated in SWC. Instead, we suggest to use the &lt;a href=&quot;http://sw.deri.org/2005/08/conf/cfp&quot;&gt;CfP ontology&lt;/a&gt;.&lt;/p&gt;
			
		</rdfs:comment>
        <vs:term_status>deprecated</vs:term_status>
        <rdfs:range rdf:resource="&swc;AcademicEvent"/>
        <rdfs:domain rdf:resource="&swc;Call"/>
        <owl:inverseOf rdf:resource="&swc;hasCall"/>
    </owl:ObjectProperty>
    


    <!-- http://data.semanticweb.org/ns/swc/ontology#hasAttendee -->

    <owl:ObjectProperty rdf:about="&swc;hasAttendee">
        <rdfs:label>has attendee</rdfs:label>
        <owl:deprecated rdf:datatype="&xsd;boolean">true</owl:deprecated>
        <rdfs:comment>
			&lt;p&gt;Instead of this property, use the &lt;a href=&quot;#Delegate&quot;&gt;Delegate&lt;/a&gt; role.&lt;/p&gt;
			
		</rdfs:comment>
        <vs:term_status>deprecated</vs:term_status>
        <rdfs:domain rdf:resource="&swc;OrganisedEvent"/>
        <rdfs:range rdf:resource="&foaf;Person"/>
    </owl:ObjectProperty>
    


    <!-- http://data.semanticweb.org/ns/swc/ontology#hasCall -->

    <owl:ObjectProperty rdf:about="&swc;hasCall">
        <rdfs:label>has call</rdfs:label>
        <owl:deprecated rdf:datatype="&xsd;boolean">true</owl:deprecated>
        <vs:term_status>deprecated</vs:term_status>
        <rdfs:comment>
			&lt;p&gt;The call classes have been deprecated in SWC. Instead, we suggest to use the &lt;a href=&quot;http://sw.deri.org/2005/08/conf/cfp&quot;&gt;CfP ontology&lt;/a&gt;.&lt;/p&gt;
			
		</rdfs:comment>
        <rdfs:domain rdf:resource="&swc;AcademicEvent"/>
        <rdfs:range rdf:resource="&swc;Call"/>
    </owl:ObjectProperty>
    


    <!-- http://data.semanticweb.org/ns/swc/ontology#hasLocation -->

    <owl:ObjectProperty rdf:about="&swc;hasLocation">
        <rdfs:label>has location</rdfs:label>
        <vs:term_status>testing</vs:term_status>
        <rdfs:comment>
			&lt;p&gt;
				Relates an event such as a conference or any sub-event thereof to a location where it takes place. 
			&lt;/p&gt;
			
		</rdfs:comment>
        <rdfs:domain rdf:resource="&swc;OrganisedEvent"/>
        <rdfs:range rdf:resource="&geo;SpatialThing"/>
    </owl:ObjectProperty>
    


    <!-- http://data.semanticweb.org/ns/swc/ontology#hasMenu -->

    <owl:ObjectProperty rdf:about="&swc;hasMenu">
        <rdfs:label>has menu</rdfs:label>
        <owl:deprecated rdf:datatype="&xsd;boolean">true</owl:deprecated>
        <vs:term_status>deprecated</vs:term_status>
        <rdfs:comment>
			&lt;p&gt;Deprecated because it has never been used and in order to reduce the inflation of ontology terms.&lt;/p&gt;
			
		</rdfs:comment>
        <rdfs:domain rdf:resource="&swc;MealEvent"/>
        <rdfs:range rdf:resource="&wordnet;Menu"/>
    </owl:ObjectProperty>
    


    <!-- http://data.semanticweb.org/ns/swc/ontology#hasPart -->

    <owl:ObjectProperty rdf:about="&swc;hasPart">
        <rdfs:label>has part</rdfs:label>
        <rdfs:comment>
			&lt;p&gt;
				A generic property to model part-of relationships. In SWC this property is used to relate a &lt;a href=&quot;#Proceedings&quot;&gt;proceedings document&lt;/a&gt; to the papers and articles contained in it.
			&lt;/p&gt;
			
		</rdfs:comment>
        <vs:term_status>testing</vs:term_status>
        <rdfs:range rdf:resource="&owl;Thing"/>
        <rdfs:domain rdf:resource="&owl;Thing"/>
    </owl:ObjectProperty>
    


    <!-- http://data.semanticweb.org/ns/swc/ontology#hasProgramme -->

    <owl:ObjectProperty rdf:about="&swc;hasProgramme">
        <rdfs:label>has programme</rdfs:label>
        <vs:term_status>testing</vs:term_status>
        <rdfs:comment>
			&lt;p&gt;
				Relates an event such as a conference to its &lt;a href=&quot;#Programme&quot;&gt;Programme&lt;/a&gt;. 
			&lt;/p&gt;
			
		</rdfs:comment>
        <rdfs:domain rdf:resource="&swc;OrganisedEvent"/>
        <rdfs:range rdf:resource="&swc;Programme"/>
    </owl:ObjectProperty>
    


    <!-- http://data.semanticweb.org/ns/swc/ontology#hasRelatedArtefact -->

    <owl:ObjectProperty rdf:about="&swc;hasRelatedArtefact">
        <rdfs:label>has related artefact</rdfs:label>
        <vs:term_status>testing</vs:term_status>
        <rdfs:comment>
			&lt;p&gt;
				Relates an &lt;a href=&quot;#AcademicEvent&quot;&gt;event&lt;/a&gt; such as a conference, workshop, etc. to a physical &lt;a href=&quot;#Artefact&quot;&gt;artefact&lt;/a&gt;.
			&lt;/p&gt;
			
		</rdfs:comment>
        <rdfs:domain rdf:resource="&swc;AcademicEvent"/>
        <rdfs:range rdf:resource="&swc;Artefact"/>
    </owl:ObjectProperty>
    


    <!-- http://data.semanticweb.org/ns/swc/ontology#hasRelatedDocument -->

    <owl:ObjectProperty rdf:about="&swc;hasRelatedDocument">
        <rdfs:label>has related document</rdfs:label>
        <rdfs:comment>
			&lt;p&gt;
				Relates an &lt;a href=&quot;#AcademicEvent&quot;&gt;event&lt;/a&gt; such as a conference, workshop, etc. to a &lt;a href=&quot;#ArgumentativeDocument&quot;&gt;document&lt;/a&gt;.
			&lt;/p&gt;
			
		</rdfs:comment>
        <vs:term_status>testing</vs:term_status>
        <rdfs:domain rdf:resource="&swc;AcademicEvent"/>
        <rdfs:range rdf:resource="&swc;ArgumentativeDocument"/>
        <owl:inverseOf rdf:resource="&swc;relatedToEvent"/>
    </owl:ObjectProperty>
    


    <!-- http://data.semanticweb.org/ns/swc/ontology#hasRole -->

    <owl:ObjectProperty rdf:about="&swc;hasRole">
        <rdfs:label>has role</rdfs:label>
        <rdfs:comment>
			&lt;p&gt;
				Part of the &lt;a href=&quot;#Role&quot;&gt;role&lt;/a&gt; modelling machinery of SWC. This property relates an event (e.g. a conference) to a role at the event (e.g. a conference chair).
			&lt;/p&gt;
			
		</rdfs:comment>
        <vs:term_status>testing</vs:term_status>
        <rdfs:domain rdf:resource="&swc;AcademicEvent"/>
        <rdfs:range rdf:resource="&swc;Role"/>
        <owl:inverseOf rdf:resource="&swc;isRoleAt"/>
    </owl:ObjectProperty>
    


    <!-- http://data.semanticweb.org/ns/swc/ontology#hasSponsorship -->

    <owl:ObjectProperty rdf:about="&swc;hasSponsorship">
        <rdfs:label>has sponsorship</rdfs:label>
        <owl:deprecated rdf:datatype="&xsd;boolean">true</owl:deprecated>
        <vs:term_status>deprecated</vs:term_status>
        <rdfs:comment>
			&lt;p&gt;Deprecated because it has never been used and in order to reduce the inflation of ontology terms.&lt;/p&gt;
			
		</rdfs:comment>
        <rdfs:domain rdf:resource="&swc;OrganisedEvent"/>
        <rdfs:range rdf:resource="&swc;Sponsorship"/>
    </owl:ObjectProperty>
    


    <!-- http://data.semanticweb.org/ns/swc/ontology#hasTopic -->

    <owl:ObjectProperty rdf:about="&swc;hasTopic">
        <rdfs:label>has topic</rdfs:label>
        <owl:deprecated rdf:datatype="&xsd;boolean">true</owl:deprecated>
        <vs:term_status>deprecated</vs:term_status>
        <rdfs:comment>
			&lt;p&gt;Instead of this property, use &lt;a href=&quot;http://xmlns.com/foaf/0.1/topic&quot;&gt;foaf:topic&lt;/a&gt;.&lt;/p&gt;
			
		</rdfs:comment>
        <rdfs:range rdf:resource="&bibtex;ResearchTopic"/>
    </owl:ObjectProperty>
    


    <!-- http://data.semanticweb.org/ns/swc/ontology#heldBy -->

    <owl:ObjectProperty rdf:about="&swc;heldBy">
        <rdfs:label>held by</rdfs:label>
        <rdfs:comment>
			&lt;p&gt;
				Part of the &lt;a href=&quot;#Role&quot;&gt;role&lt;/a&gt; modelling machinery of SWC. This property relates a role at an event (e.g. a conference chair) to the person who actually holds the role.
			&lt;/p&gt;
			
		</rdfs:comment>
        <vs:term_status>testing</vs:term_status>
        <rdfs:domain rdf:resource="&swc;Role"/>
        <owl:inverseOf rdf:resource="&swc;holdsRole"/>
        <rdfs:range rdf:resource="&foaf;Person"/>
    </owl:ObjectProperty>
    


    <!-- http://data.semanticweb.org/ns/swc/ontology#holdsRole -->

    <owl:ObjectProperty rdf:about="&swc;holdsRole">
        <rdfs:label>holds role</rdfs:label>
        <rdfs:comment>
			&lt;p&gt;
				Part of the &lt;a href=&quot;#Role&quot;&gt;role&lt;/a&gt; modelling machinery of SWC. This property relates a person to the role (e.g. a conference chair) it holds at an event.
			&lt;/p&gt;
			
		</rdfs:comment>
        <vs:term_status>testing</vs:term_status>
        <rdfs:range rdf:resource="&swc;Role"/>
        <rdfs:domain rdf:resource="&foaf;Person"/>
    </owl:ObjectProperty>
    


    <!-- http://data.semanticweb.org/ns/swc/ontology#isLocationFor -->

    <owl:ObjectProperty rdf:about="&swc;isLocationFor">
        <rdfs:label>is location of</rdfs:label>
        <rdfs:comment>
			&lt;p&gt;
				Relates a location to a conference or sub-event thereof which takes place at it.
			&lt;/p&gt;
			
		</rdfs:comment>
        <vs:term_status>testing</vs:term_status>
        <rdfs:range rdf:resource="&swc;OrganisedEvent"/>
        <rdfs:domain rdf:resource="&geo;SpatialThing"/>
    </owl:ObjectProperty>
    


    <!-- http://data.semanticweb.org/ns/swc/ontology#isPartOf -->

    <owl:ObjectProperty rdf:about="&swc;isPartOf">
        <rdfs:label>is part of</rdfs:label>
        <vs:term_status>testing</vs:term_status>
        <rdfs:comment>
			&lt;p&gt;
				A generic property to model part-of relationships. In SWC this property is used to relate papers or articles to the &lt;a href=&quot;#Proceedings&quot;&gt;proceedings document&lt;/a&gt; in which they are contained.
			&lt;/p&gt;
			
		</rdfs:comment>
        <rdfs:range rdf:resource="&owl;Thing"/>
        <rdfs:domain rdf:resource="&owl;Thing"/>
    </owl:ObjectProperty>
    


    <!-- http://data.semanticweb.org/ns/swc/ontology#isProvidedBy -->

    <owl:ObjectProperty rdf:about="&swc;isProvidedBy">
        <rdfs:label>is provided by</rdfs:label>
        <owl:deprecated rdf:datatype="&xsd;boolean">true</owl:deprecated>
        <rdfs:comment>
			&lt;p&gt;Deprecated because it has never been used and in order to reduce the inflation of ontology terms.&lt;/p&gt;
			
		</rdfs:comment>
        <vs:term_status>deprecated</vs:term_status>
        <rdfs:domain rdf:resource="&swc;Sponsorship"/>
        <rdfs:range rdf:resource="&foaf;Organisation"/>
    </owl:ObjectProperty>
    


    <!-- http://data.semanticweb.org/ns/swc/ontology#isProviderOf -->

    <owl:ObjectProperty rdf:about="&swc;isProviderOf">
        <rdfs:label>is provider of</rdfs:label>
        <owl:deprecated rdf:datatype="&xsd;boolean">true</owl:deprecated>
        <vs:term_status>deprecated</vs:term_status>
        <rdfs:comment>
			&lt;p&gt;Deprecated because it has never been used and in order to reduce the inflation of ontology terms.&lt;/p&gt;
			
		</rdfs:comment>
        <rdfs:range rdf:resource="&swc;Sponsorship"/>
        <owl:inverseOf rdf:resource="&swc;isProvidedBy"/>
        <rdfs:domain rdf:resource="&foaf;Organisation"/>
    </owl:ObjectProperty>
    


    <!-- http://data.semanticweb.org/ns/swc/ontology#isRoleAt -->

    <owl:ObjectProperty rdf:about="&swc;isRoleAt">
        <rdfs:label>is role at</rdfs:label>
        <rdfs:comment>
			&lt;p&gt;
				Part of the &lt;a href=&quot;#Role&quot;&gt;role&lt;/a&gt; modelling machinery of SWC. This property relates a role at an event (e.g. a conference chair) to the event (e.g. a conference).
			&lt;/p&gt;
			
		</rdfs:comment>
        <vs:term_status>testing</vs:term_status>
        <rdfs:range rdf:resource="&swc;AcademicEvent"/>
        <rdfs:domain rdf:resource="&swc;Role"/>
    </owl:ObjectProperty>
    


    <!-- http://data.semanticweb.org/ns/swc/ontology#isSubEventOf -->

    <owl:ObjectProperty rdf:about="&swc;isSubEventOf">
        <rdf:type rdf:resource="&owl;TransitiveProperty"/>
        <rdfs:label>is sub-event of</rdfs:label>
        <rdfs:label>is a sub-event of</rdfs:label>
        <rdfs:comment>Events can be sub-events of other events. E.g., a talk can be a sub-event of a session, which in turn can be a sub-event of a conference. The relation is transitive - the talk is also a sub-event of the conference.</rdfs:comment>
        <vs:term_status>testing</vs:term_status>
        <owl:inverseOf rdf:resource="&swc;isSuperEventOf"/>
        <rdfs:range rdf:resource="&ical;Vevent"/>
        <rdfs:domain rdf:resource="&ical;Vevent"/>
    </owl:ObjectProperty>
    


    <!-- http://data.semanticweb.org/ns/swc/ontology#isSuperEventOf -->

    <owl:ObjectProperty rdf:about="&swc;isSuperEventOf">
        <rdf:type rdf:resource="&owl;TransitiveProperty"/>
        <rdfs:label>is super-event of</rdfs:label>
        <rdfs:label>is the super-event of</rdfs:label>
        <rdfs:comment>Events can be super-events of other events. E.g., a conference can be the super-event of a session, which in turn can be the super-event of a talk. The relation is transitive - the conference is also the super-event of the talk.</rdfs:comment>
        <vs:term_status>testing</vs:term_status>
        <rdfs:domain rdf:resource="&ical;Vevent"/>
        <rdfs:range rdf:resource="&ical;Vevent"/>
    </owl:ObjectProperty>
    


    <!-- http://data.semanticweb.org/ns/swc/ontology#memberOf -->

    <owl:ObjectProperty rdf:about="&swc;memberOf">
        <rdfs:label>member of</rdfs:label>
        <vs:term_status>testing</vs:term_status>
        <rdfs:comment>
			&lt;p&gt;FOAF does not define an inverse of &lt;code&gt;foaf:member&lt;/code&gt;, so SWC does it.&lt;/p&gt;
			
		</rdfs:comment>
        <rdfs:range rdf:resource="&foaf;Group"/>
        <rdfs:domain rdf:resource="&foaf;Person"/>
        <owl:inverseOf rdf:resource="&foaf;member"/>
    </owl:ObjectProperty>
    


    <!-- http://data.semanticweb.org/ns/swc/ontology#plansToAttend -->

    <owl:ObjectProperty rdf:about="&swc;plansToAttend">
        <rdfs:label>plans to attend</rdfs:label>
        <vs:term_status>testing</vs:term_status>
        <rdfs:comment>
			&lt;p&gt;
				A person might plan to attend a talk or session at a conference or similar event. This property could be used to prepare a delegate&apos;s conference experience with respect to who they want to meet, etc.
			&lt;/p&gt;
			
		</rdfs:comment>
        <rdfs:range rdf:resource="&swc;OrganisedEvent"/>
        <rdfs:domain rdf:resource="&foaf;Person"/>
    </owl:ObjectProperty>
    


    <!-- http://data.semanticweb.org/ns/swc/ontology#relatedToEvent -->

    <owl:ObjectProperty rdf:about="&swc;relatedToEvent">
        <rdfs:label>related to event</rdfs:label>
        <rdfs:label>related to Event</rdfs:label>
        <rdfs:comment>
			&lt;p&gt;
				Relates a &lt;a href=&quot;#ArgumentativeDocument&quot;&gt;document&lt;/a&gt; to an &lt;a href=&quot;#AcademicEvent&quot;&gt;event&lt;/a&gt; such as a conference, workshop, etc.
			&lt;/p&gt;
			
		</rdfs:comment>
        <vs:term_status>testing</vs:term_status>
        <rdfs:range rdf:resource="&swc;AcademicEvent"/>
        <rdfs:domain rdf:resource="&swc;ArgumentativeDocument"/>
        <rdfs:domain rdf:resource="&swc;Artefact"/>
